package tester;

import java.util.Scanner;

public class StackTest {
	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)){
			
			System.out.println("Stack: ");
			System.out.println("Operations\n1. Push\n2. Pop\n3. Top\n4. Get Max\n");
			switch (sc.nextInt()) {
			case 1:
				System.out.println("Push operations: ");
				break;
			case 2:
				System.out.println("Pop operations: ");
				break;
			case 3:
				System.out.println("Top operations: ");
				break;
			case 4:
				System.out.println("Get Max operations: ");
				break;
			default: 
				System.out.println("Invalid Options");
				break;
			}
			
		}
		catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
	}
}
